{
    "manifest_version": 3,
    "name": "Volkers Replacer",
    "version": "1.0",
    "description": "Replaces #F5F7FA with #9db0cd on any website.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  